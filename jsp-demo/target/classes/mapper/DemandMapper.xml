<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jsp.mapper.DemandMapper" >
    <select id="getByIdForMap" parameterType="long" resultType="java.util.Map">
      select * from `c_business_demand` `cbd`  where `cbd`.`id` = #{id}
    </select>
</mapper>